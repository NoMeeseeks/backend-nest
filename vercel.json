{
    "version": 2,
    "env": {
        "MONGO_URI" : "",
        "MONGO_DB_NAME" : "",
        "JWT_SEED":""
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}